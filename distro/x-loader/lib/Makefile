include $(TOPDIR)/config.mk

LIB	= lib$(ARCH).a

AOBJS	= _udivsi3.o _umodsi3.o

COBJS	= board.o ecc_256.o ecc_512.o printf.o div0.o

OBJS	= $(AOBJS) $(COBJS)

$(LIB):	.depend $(OBJS)
	$(AR) crv $@ $(OBJS)

#########################################################################

sinclude .depend

